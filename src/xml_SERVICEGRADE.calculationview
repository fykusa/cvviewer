        <calculatedViewAttribute datatype="VARCHAR" id="VL_STATUS" length="1" expressionLanguage="COLUMN_ENGINE">
          <formula>if(not(isNull(&quot;VR_SUM_FKIMG&quot;)) and &quot;VR_SUM_FKIMG&quot;>=&quot;VL_LFIMG&quot;,'Q', if(not(isNull(&quot;MD_SUM_MENGE&quot;)) and &quot;MD_SUM_MENGE&quot;>=&quot;VL_LFIMG&quot;,'P',&quot;VL_IN_STATUS&quot;))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="VL_STATUS_TS" length="14" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;VL_STATUS&quot;='B',&quot;VL_TS&quot;,&#xD;
if(&quot;VL_STATUS&quot;='C',&quot;LT_TS&quot;,&#xD;
if(&quot;VL_STATUS&quot;='D',&quot;LT_QTS&quot;,    &#xD;
if(in(&quot;VL_STATUS&quot;, 'E','F','G','H','I','J','K','L','M','N','O'),&quot;LP_MAX_TS&quot;,         &#xD;
if(&quot;VL_STATUS&quot;='P',&quot;MD_TS&quot;,     &#xD;
if(&quot;VL_STATUS&quot;='Q',&quot;VR_TS&quot;,  ''&#xD;
))))))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="VL_LP_ROUTE" length="6" expressionLanguage="COLUMN_ENGINE">
          <formula>if( isnull(&quot;LP_ROUTE&quot;),&quot;VL_ROUTE&quot;,&quot;LP_ROUTE&quot;)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="VLC_LGORT_E_W_VL_LT_LP" length="4" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;VL_STATUS&quot;='B',&quot;VL_LGORT&quot;,&#xD;
if(&quot;VL_STATUS&quot;='C',if(&quot;LTC_SOURCE&quot;='K' or &quot;LT_LGNUM&quot;='',&quot;VL_LGORT&quot;,'0'+&quot;LT_LGNUM&quot;),&#xD;
if(&quot;VL_STATUS&quot;='D',if(&quot;LTC_SOURCE&quot;='K' or &quot;LT_LGNUM&quot;='',&quot;VL_LGORT&quot;,'0'+&quot;LT_LGNUM&quot;),   &#xD;
if(in(&quot;VL_STATUS&quot;, 'E','F','G','H','I','J','K','L','M','N','O') and &quot;LP_LWRK&quot;!='' and &quot;LP_LWRK&quot;!='00' ,'00'+&quot;LP_LWRK&quot;,         &#xD;
if(&quot;VL_STATUS&quot;='P' and &quot;LP_LWRK&quot;!=''  and &quot;LP_LWRK&quot;!='00' ,'00'+&quot;LP_LWRK&quot;,     &#xD;
if(&quot;VL_STATUS&quot;='Q' and &quot;LP_LWRK&quot;!='' and &quot;LP_LWRK&quot;!='00'  ,'00'+&quot;LP_LWRK&quot;,  &quot;VL_LGORT&quot;&#xD;
))))))&#xD;
</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
