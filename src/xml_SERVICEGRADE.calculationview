      &quot;XX_STATUS_TS&quot; &lt; &quot;TG_DUEGI&quot;&#xD;
      )&#xD;
      and&#xD;
      (&quot;VAC_ABGR_TS_CORR&quot; = '' or&#xD;
       isNull(&quot;VAC_ABGR_TS_CORR&quot;) or &#xD;
       &quot;VA_ABGR_TS&quot; > &quot;TG_DUEGI&quot;)),&#xD;
  1,0)</formula>
        </calculatedViewAttribute>
