      <calculatedViewAttributes>
        <calculatedViewAttribute datatype="VARCHAR" id="VL_STATUS" length="1" expressionLanguage="SQL">
          <formula>case when (&quot;LTC_SOURCE&quot; = 'K')&#xD;
then &#xD;
  case when (&quot;KO_SA_STATUS&quot; >= '191' AND &quot;KO_SA_STATUS&quot; &lt;='208')&#xD;
  then &#xD;
     case when (&quot;KO_PI_GESNDT_P&quot; = 'N' AND &quot;KO_TMENG_P&quot; > 0)&#xD;
       then 'D'&#xD;
        else 'C'&#xD;
        end&#xD;
      else&#xD;
        case when (&quot;LT_TANUM&quot; &lt;> '' and &quot;LT_TANUM&quot; is not null)&#xD;
        then 'C'&#xD;
     else 'B'&#xD;
     end&#xD;
  end&#xD;
 else &#xD;
 case when ( &quot;LT_VSOLM_CORRECTED&quot; is not null and &quot;LT_VSOLM_CORRECTED&quot;>=&quot;VL_LFIMG&quot;)&#xD;
     then&#xD;
     case when ( &quot;LT_NISTM_CORRECTED&quot; is not null and &quot;LT_NISTM_CORRECTED&quot;>=&quot;VL_LFIMG&quot;)&#xD;
     then&#xD;
   'D'&#xD;
       else 'C'&#xD;
       end&#xD;
      else 'B' &#xD;
       end&#xD;
  end</formula>
