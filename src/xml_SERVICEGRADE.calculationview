      <calculatedViewAttributes>
        <calculatedViewAttribute datatype="VARCHAR" id="VA_STATUS" length="2" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;VA_ABGRU&quot;='', &#xD;
  if(&quot;VA_LIFSK&quot;='', &#xD;
    if(&quot;VA_LIFSP_YN&quot;='',&#xD;
      if(&quot;VA_KBMENG&quot; >= &quot;VA_KWMENG&quot;,&#xD;
          if(isNull(&quot;FI_AUFNR&quot;) or &quot;FI_AUFNR&quot; = '','A',&#xD;
             if((instr(&quot;FI_STATUS_T&quot;,'KKMP')=0 and ( instr(&quot;FI_STATUS_T&quot;,'DRUC')!=0 ) and (instr(&quot;FI_STATUS_T&quot;,'WABE')!=0)) and &quot;FI_AUFNR&quot; != '','2','1')),&#xD;
                if(&quot;VA_ZZ_BMENGSP&quot; + &quot;VA_KBMENG&quot; &lt;  &quot;VA_KWMENG&quot; ,'0','5')),&#xD;
      '-'),&#xD;
    '%'),&#xD;
'*')&#xD;
&#xD;
</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="VA_STATUS_TS" length="14" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;VA_STATUS&quot;='1' or  &quot;VA_STATUS&quot;='2',&quot;VAC_FI_ORDER_TS&quot;,&#xD;
if(&quot;VA_STATUS&quot;='A' or &quot;VA_STATUS&quot;='5',&quot;VA_1ST_COMMIT_TS&quot;,if(&quot;VA_STATUS&quot;='0',&quot;VAC_STATUS_0_TS&quot;,&quot;VA_TS&quot;)))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="VAC_FI_ORDER_TS" length="14" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;FI_DATUM&quot;+&quot;FI_UZEIT&quot;</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="VAC_RELEVANT_QTY_FIB" length="17" scale="3" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;VA_RELEVANT_QTY&quot;</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="VAC_COMMITED_QTY_FIB" length="17" scale="3" expressionLanguage="COLUMN_ENGINE">
          <formula> &quot;VA_RELEVANT_COMMITTED_QTY&quot;</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="VAC_LGORT_WITH_FI" length="4" expressionLanguage="COLUMN_ENGINE">
          <formula>if((&quot;VA_STATUS&quot;='1' or &quot;VA_STATUS&quot;='2')  and &quot;FI_LGNUM_P&quot;!='',&quot;FI_LGNUM_P&quot;,&quot;VAC_LGORT&quot;)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="VLC_CNT_SGR_REL" expressionLanguage="COLUMN_ENGINE">
          <formula>if ( (( &#xD;
      &quot;VA_LOCK_TS&quot;&lt; &quot;TG_DUEGI&quot; or&#xD;
      &quot;VA_1ST_COMMIT_TS&quot; &lt; &quot;TG_DUEGI&quot; or&#xD;
      &quot;VAC_STATUS_0_TS&quot; &lt; &quot;TG_DUEGI&quot; or&#xD;
      &quot;TG_ORDERTS&quot; &lt; &quot;TG_DUEGI&quot; or&#xD;
      &quot;VA_STATUS_TS&quot; &lt; &quot;TG_DUEGI&quot;)&#xD;
      and&#xD;
      (&quot;VAC_ABGR_TS_CORR&quot; = '' or&#xD;
       isNull(&quot;VAC_ABGR_TS_CORR&quot;) or &#xD;
       &quot;VAC_ABGR_TS_CORR&quot; > &quot;TG_DUEGI&quot;)),&#xD;
  1,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="BIGINT" id="VLC_CNT_BGR" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;VA_STATUS&quot;='0',0,&#xD;
  if(in(&quot;VA_STATUS&quot;,'1','2','5','A') and &quot;VLC_CNT_SGR_REL&quot; = 1 and &quot;VA_1ST_COMMIT_TS&quot; &lt;= &quot;TG_DUEGI&quot;,1,0))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="FIC_FBLNR_FINAL" length="3" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;FI_VWK_EIS_DEL&quot; = 'X','',&quot;FI_FBLNR&quot;)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="VAC_CONF_VALUE" length="1" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(&quot;VLC_CNT_SGR_REL&quot; = 0,&#xD;
  '',&#xD;
  IF(&quot;VAC_VACTS&quot; = '' OR &quot;VAC_VACTS&quot; = '00000000000000' OR isNull(&quot;VAC_VACTS&quot;),&#xD;
    '0',&#xD;
    IF(&quot;VAC_VACTS&quot; &lt;= &quot;TG_ORDEREND&quot;,&#xD;
      '1',&#xD;
      IF(&quot;TG_ORDEREND&quot; &lt; &quot;VAC_VACTS&quot; AND &quot;VAC_VACTS&quot; &lt;= &quot;TG_DUEOC&quot;,&#xD;
        '2',&#xD;
        IF(&quot;TG_DUEOC&quot; &lt; &quot;VAC_VACTS&quot;,&#xD;
          '3',&#xD;
          '4'&#xD;
        )&#xD;
      )&#xD;
    )&#xD;
  )&#xD;
)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="VAC_TARGET_VALUE" length="1" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(&quot;VLC_CNT_SGR_REL&quot; = 1, '0', '')</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
